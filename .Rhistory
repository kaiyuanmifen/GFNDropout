VecPlot=Data[(Data$Data==DataSet)&(Data$Epoch<200)&(Data$p==p)&(Data$N_units==N_units),]
p <- ggplot(VecPlot, aes(x=Epoch, y=test_acc,color=Method)) +geom_smooth()+
ggtitle(NAME)
p
ggsave(paste0('images/',NAME,"_testacc.png"),scale=3)
}
}
}
